adams.flow.control.Flow -annotation "Plots statistics from a Detectron2 metrics.json file." -flow-execution-listener adams.flow.execution.NullListener -flow-restart-manager adams.flow.control.flowrestart.NullManager
 adams.flow.standalone.CallableActors
  adams.flow.sink.SequencePlotter -name Statistics -short-title true -display-type adams.flow.core.displaytype.Default -width @{plot_width} -height @{plot_height} -writer adams.gui.print.NullWriter -comparison X_AND_Y -paintlet "adams.gui.visualization.sequence.LinePaintlet -markers-disabled true -paint-all true" -overlay-paintlet adams.gui.visualization.sequence.NullPaintlet -marker-paintlet adams.flow.sink.sequenceplotter.NoMarkers -error-paintlet adams.flow.sink.sequenceplotter.NoErrorPaintlet -mouse-click-action adams.flow.sink.sequenceplotter.NullClickAction -color-provider @{color_provider} -overlay-color-provider adams.gui.visualization.core.DefaultColorProvider -title Statistics -axis-x "adams.gui.visualization.core.AxisPanelOptions -label evaluations -tick-generator \"adams.gui.visualization.core.axis.FancyTickGenerator -num-ticks 20\" -width 40 -custom-format 0" -axis-y "adams.gui.visualization.core.AxisPanelOptions -label value -tick-generator adams.gui.visualization.core.axis.FancyTickGenerator -width 60 -custom-format 0.000" -side-panel-width @{side_panel_width} -plot-updater "adams.flow.sink.sequenceplotter.SimplePlotUpdater -update-interval @{update_interval}" -post-processor adams.flow.sink.sequenceplotter.PassThrough
 adams.flow.source.Start
 adams.flow.control.Trigger -name "prompt user"
  adams.flow.standalone.SetVariable -name "restore file" -var-name restore -var-value @{flow_filename_long}.props -value-type FILE_FORWARD_SLASHES -expand-value true
  adams.flow.source.EnterManyValues -name Parameters -stop-if-canceled true -value "adams.flow.source.valuedefinition.FileValueDefinition -name input_file -display \"Input file\" -help \"The metrics.json file to use for plotting\" -file-chooser-title \"Input file\" -extension json -use-forward-slashes true" -value "adams.flow.source.valuedefinition.GOEValueDefinition -name color_provider -display \"Color provider\" -help \"How to colorize the plots\" -super-class adams.gui.visualization.core.ColorProvider -default-class adams.gui.visualization.core.DefaultColorProvider" -value "adams.flow.source.valuedefinition.DefaultValueDefinition -name side_panel_width -display \"Side panel width\" -help \"The width of the side panel in the plot\" -type INTEGER -default-value 250" -value "adams.flow.source.valuedefinition.DefaultValueDefinition -name plot_width -display \"Plot width\" -help \"The width of the plot window\" -type INTEGER -default-value 800" -value "adams.flow.source.valuedefinition.DefaultValueDefinition -name plot_height -display \"Plot height\" -help \"The height of the plot window\" -type INTEGER -default-value 350" -value "adams.flow.source.valuedefinition.DefaultValueDefinition -name update_interval -display \"Update interval\" -help \"After how many data points to update the graph, use -1 to only update once\" -type INTEGER -default-value 1000" -output-type MAP -restoration-enabled true -restoration-file @{restore}
  adams.flow.transformer.MapToVariables
 adams.flow.control.Trigger -name "read data"
  adams.flow.source.FileSupplier -file @{input_file}
  adams.flow.transformer.TextFileReader -reader adams.data.io.input.LineArrayTextReader
  adams.flow.transformer.SetStorageValue -storage-name data
 adams.flow.control.Trigger -name "select stats"
  adams.flow.source.StorageValue -storage-name data -conversion adams.data.conversion.UnknownToUnknown
  adams.flow.transformer.GetArrayElement
  adams.flow.transformer.Convert -conversion adams.data.conversion.StringToJson
  adams.flow.transformer.GetJsonKeys -output-array true -sort-keys true
  adams.flow.transformer.StringMatcher -regexp (data_time|eta_seconds) -invert true
  adams.flow.transformer.SelectArraySubset
  adams.flow.transformer.SetStorageValue -storage-name stats
 adams.flow.control.Trigger -name plot
  adams.flow.source.StorageValue -storage-name data -conversion adams.data.conversion.UnknownToUnknown
  adams.flow.transformer.ArrayToSequence
  adams.flow.transformer.Convert -conversion adams.data.conversion.StringToJson
  adams.flow.transformer.SetStorageValue -storage-name item
  adams.flow.control.Trigger -name "iterate stats"
   adams.flow.source.StorageValue -storage-name stats -conversion adams.data.conversion.UnknownToUnknown
   adams.flow.transformer.ArrayToSequence
   adams.flow.transformer.SetVariable -var-name stat
   adams.flow.control.Trigger -name "plot stat"
    adams.flow.source.StorageValue -storage-name item -conversion adams.data.conversion.UnknownToUnknown
    adams.flow.transformer.GetJsonValue -path @{stat} -data-type NUMBER
    adams.flow.transformer.MakePlotContainer -plot-name @{stat}
    adams.flow.sink.CallableSink -callable Statistics